     <ooclass>
      <classname>Ds\Stack</classname>
     </ooclass>

     <oointerface>
      <interfacename>Ds\Collection</interfacename>
     </oointerface>
     <oointerface>
      <interfacename>ArrayAccess</interfacename>
     </oointerface>
    </classsynopsisinfo>
    <!-- }}} -->

    <classsynopsisinfo role="comment">&Methods;</classsynopsisinfo>
