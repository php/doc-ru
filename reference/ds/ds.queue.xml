     <ooclass>
      <classname>Ds\Queue</classname>
     </ooclass>

     <oointerface>
      <interfacename>Ds\Collection</interfacename>
     </oointerface>
    </classsynopsisinfo>
<!-- }}} -->
    <classsynopsisinfo role="comment">Константы</classsynopsisinfo>
