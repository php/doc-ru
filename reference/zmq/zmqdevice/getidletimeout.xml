 <refnamediv>
  <refname>ZMQDevice::getIdleTimeout</refname>
  <refpurpose>Получить таймаут бездействия</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>ZMQDevice</type><methodname>ZMQDevice::getIdleTimeout</methodname>
   <void/>
