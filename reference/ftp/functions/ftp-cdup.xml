  <refentry id="function.ftp-cdup">
   <refnamediv>
    <refname>ftp_cdup</refname>
    <refpurpose>Переходит в родительскую директорию</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Описание</title>
     <methodsynopsis>
      <type>bool</type><methodname>ftp_cdup</methodname>
      <methodparam><type>resource</type><parameter>ftp_stream</parameter></methodparam>
     </methodsynopsis>
    <para> 
     Переходит в родительскую директорию.
    </para>
    <para>
     <example>
      <title>Пример использования функции <function>ftp_cdup</function></title>
      <programlisting role="php">
<![CDATA[
<?php
// установка соединения
$conn_id = ftp_connect($ftp_server);

// проверка имени пользователя и пароля
$login_result = ftp_login($conn_id, $ftp_user_name, $ftp_user_pass);

// установка html в качестве текущей директории
ftp_chdir($conn_id, 'html');

echo ftp_pwd($conn_id); // /html 

// возврат в родительскую директорию
if (ftp_cdup($conn_id)) { 
  echo "команда cdup выполнена успешно\n";
} else {
  echo "команда cdup завергилась неудачно\n";
}

echo ftp_pwd($conn_id); // /

ftp_close($conn_id);
?>
]]>
      </programlisting>
     </example>
    </para>
    <para>
     &return.success;
    </para>
    <para>
     См. также описание функции <function>ftp_chdir</function>.
    </para>
   </refsect1>
  </refentry>

