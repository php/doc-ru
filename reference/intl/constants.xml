    </listitem>
   </varlistentry>
   <varlistentry xml:id="constant.idna-error-leading-combining-mark">
    <term>
     <constant>IDNA_ERROR_LEADING_COMBINING_MARK</constant>
     (<type>int</type>)
    </term>
    <listitem>
     <simpara>
