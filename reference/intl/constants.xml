     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="constant.idna-error-punycode">
    <term>
     <constant>IDNA_ERROR_PUNYCODE</constant>
     (<type>int</type>)
    </term>
    <listitem>
     <simpara>
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry xml:id="constant.idna-error-label-has-dot">
    <term>
     <constant>IDNA_ERROR_LABEL_HAS_DOT</constant>
     (<type>int</type>)
    </term>
    <listitem>
