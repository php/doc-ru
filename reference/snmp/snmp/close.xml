  <example xml:id="snmp.close.example.basic">
   <title>Пример использования <methodname>SNMP::close</methodname></title>
   <para>
   </para>
   <programlisting role="php">
<![CDATA[
<?php
  $session = new SNMP(SNMP::VERSION_1, "127.0.0.1", "public");
  # ...
  # получить, что-то сделать и т.д.
  # ...
  $session->close();
?>
]]>
   </programlisting>
